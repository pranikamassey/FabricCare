                                        <%OrganList.Kidney.forEach(function(kidney){%>
                                            <tr>
                                            <td><%=kidney.organId%></td>
                                            <td><%=kidney.hospital%></td>
                                            <td><%=kidney.date%></td>
                                            <td><%=kidney.time%>mdo</td>
                                            <td><%=kidney.solution%></td>
